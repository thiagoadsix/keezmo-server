{
  "name": "keezmo",
  "version": "1.0.0",
  "description": "API for Keezmo",
  "main": "src/index.ts",
  "scripts": {
    "build": "tsc",
    "test": "vitest run --config vitest.config.mts",
    "test:watch": "vitest",
    "test:types": "vitest run --typecheck",
    "test:coverage": "vitest run --coverage",
    "test:full": "vitest run --typecheck --coverage",
    "start": "node dist/index.js",
    "dev": "nodemon",
    "prepare": "husky",
    "commit": "git add . && git-cz",
    "release": "standard-version",
    "lint:fix": "eslint --fix 'src/**/*.{js,jsx,ts,tsx}' --quiet"
  },
  "keywords": [
    "keezmo",
    "node",
    "typescript",
    "aws",
    "api",
    "rest",
    "serverless"
  ],
  "author": "Thiago Andrade Silva",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.788.0",
    "@aws-sdk/lib-dynamodb": "^3.788.0",
    "@middy/core": "^6.1.6",
    "aws-cdk-lib": "^2.189.0",
    "constructs": "^10.4.2"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^19.5.0",
    "@eslint/compat": "^1.2.8",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.24.0",
    "@rocketseat/eslint-config": "^2.2.2",
    "@serverless/typescript": "^4.11.1",
    "@types/aws-lambda": "^8.10.149",
    "@types/node": "^22.14.1",
    "@typescript-eslint/parser": "^8.29.1",
    "@vitest/coverage-v8": "^3.1.1",
    "commitizen": "^4.3.1",
    "commitlint": "^19.5.0",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^7.0.0",
    "eslint": "^9.24.0",
    "eslint-import-resolver-typescript": "^4.3.2",
    "eslint-plugin-import": "^2.31.0",
    "globals": "^16.0.0",
    "husky": "^9.1.6",
    "nodemon": "^3.1.4",
    "standard-version": "^9.5.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.29.1",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.1.1"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  }
}