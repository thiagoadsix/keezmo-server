{
  "name": "keezmo",
  "version": "4.0.0",
  "description": "API for Keezmo",
  "main": "src/main/serverless.ts",
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "test": "vitest run --config vitest.config.mts --silent",
    "test:watch": "vitest",
    "test:types": "vitest run --typecheck",
    "test:coverage": "vitest run --coverage",
    "test:full": "vitest run --typecheck --coverage",
    "test:scope": "tsx scripts/test-scope.ts",
    "start": "node dist/index.js",
    "dev": "dotenv -- nodemon",
    "prepare": "husky",
    "commit": "git-cz",
    "release": "standard-version",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "lint:fix": "eslint --fix '{src,__tests__}/**/*.{js,jsx,ts,tsx}'"
  },
  "keywords": [
    "keezmo",
    "node",
    "typescript",
    "aws",
    "api",
    "rest",
    "serverless"
  ],
  "author": "Thiago Andrade Silva",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.788.0",
    "@aws-sdk/lib-dynamodb": "^3.789.0",
    "@aws-sdk/util-dynamodb": "^3.798.0",
    "@middy/core": "^6.1.6",
    "@middy/http-json-body-parser": "^6.2.2",
    "@middy/http-security-headers": "^6.2.2",
    "aws-cdk-lib": "^2.189.0",
    "constructs": "^10.4.2",
    "jsonwebtoken": "^9.0.2",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^19.5.0",
    "@eslint/compat": "^1.2.8",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.25.1",
    "@eslint/json": "^0.12.0",
    "@eslint/markdown": "^6.4.0",
    "@rocketseat/eslint-config": "^2.2.2",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "^12.0.1",
    "@serverless/typescript": "^4.11.1",
    "@types/aws-lambda": "^8.10.149",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^22.14.1",
    "@types/prompts": "^2.4.9",
    "@typescript-eslint/parser": "^8.31.1",
    "@vitest/coverage-v8": "^3.1.1",
    "aws-sdk-client-mock": "^4.1.0",
    "aws-sdk-client-mock-vitest": "^6.1.1",
    "chalk": "^5.4.1",
    "commitizen": "^4.3.1",
    "commitlint": "^19.5.0",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^7.0.0",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9.25.1",
    "eslint-import-resolver-typescript": "^4.3.4",
    "eslint-plugin-import": "^2.31.0",
    "globals": "^16.0.0",
    "husky": "^9.1.6",
    "nodemon": "^3.1.4",
    "prompts": "^2.4.2",
    "semantic-release": "^24.2.3",
    "serverless-esbuild": "^1.55.0",
    "serverless-offline": "^14.4.0",
    "standard-version": "^9.5.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.19.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.31.1",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.1.1"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  }
}
